#include <iostream>

using namespace std;

int main() {
    int number;

    cout << "Enter a number: ";
    cin >> number;

    if (number > 0) {
        cout << "Positive";
        if (number % 2 == 0) {
            cout << " and Even" << endl;
        } else {
            cout << " and Odd" << endl;
        }
    } else if (number < 0) {
        cout << "Negative" << endl;
    } else {
        cout << "Zero" << endl;
    }

    return 0;
}
