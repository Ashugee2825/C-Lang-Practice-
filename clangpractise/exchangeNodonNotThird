#include <iostream>

using namespace std;

int main() {
    int num1, num2;

    // Input two numbers
    cout << "Enter the first number: ";
    cin >> num1;
    cout << "Enter the second number: ";
    cin >> num2;

    // Display the original values
    cout << "Original values: num1 = " << num1 << ", num2 = " << num2 << endl;

    // Exchange the values
    num1 = num1 + num2;
    num2 = num1 - num2;
    num1 = num1 - num2;

    // Display the exchanged values
    cout << "Exchanged values: num1 = " << num1 << ", num2 = " << num2 << endl;

    return 0;
}
